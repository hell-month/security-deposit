{
  "name": "@hell-month/security-deposit-sdk",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "out"
  ],
  "scripts": {
    "build": "tsc",
    "copy": "mkdir -p out && cp -r ../out/SecurityDepositPool.sol/* out",
    "typechain": "typechain --target ethers-v6 --out-dir src --input-dir out --glob **/*.json",
    "prepare": "npm run copy && ls out && npm run typechain && npm run build"
  },
  "keywords": [
    "ethereum",
    "smart-contract",
    "abi"
  ],
  "author": "9oelM",
  "license": "MIT",
  "description": "Security deposit pool contract ABI",
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "typechain": "^8.3.2",
    "typescript": "^5.9.2"
  }
}
